{
  "name": "react-tpl",
  "version": "1.0.17-1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint src/",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "dependencies": {
    "material-ui": "^1.0.0-beta.20",
    "material-ui-icons": "^1.0.0-beta.17",
    "normalize.css": "^7.0.0",
    "react": "16.0.0",
    "react-dom": "16.0.0"
  },
  "devDependencies": {
    "@types/jest": "^21.1.5",
    "@types/node": "^8.0.49",
    "@types/react": "^16.0.21",
    "@types/react-dom": "^16.0.2",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "react-app-rewired": "1.3.5",
    "react-scripts": "1.0.17",
    "react-styleguidist": "6.0.33",
    "ts-jest": "^21.1.4",
    "ts-loader": "^3.1.1",
    "tslint": "^5.8.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{tsx}",
      "!src/**/*stories.{tsx}",
      "!src/{index,registerServiceWorker,setupTests,inject-tap-event-plugin,injectEnv}.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/src/setupTests.tsx"
    ],
    "transform": {
      "^.+\\.css$": "<rootDir>/node_modules/react-scripts/config/jest/cssTransform.js",
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^(?!.*\\.(ts|tsx|js|jsx|css|json)$)": "<rootDir>/node_modules/react-scripts/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(ts|tsx|js|jsx)$"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|tsx|js|jsx)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|tsx|js|jsx)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "testURL": "http://localhost"
  }
}
