{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": false,
        "arrow-parens": false,
        "interface-name": false,
        "no-console": [
          true,
          "debug",
          "info",
          "time",
          "timeEnd",
          "trace"
        ],
        "member-access": false,
        "indent": [
          true,
          "spaces"
        ],
        "jsx-boolean-value": false,
        "jsx-no-lambda": false,
        "jsx-no-multiline-js": false,
        "max-line-length": [
          true,
          140
        ],
        "quotemark": [
          true,
          "single",
          "jsx-double",
          "avoid-escape"
        ],
        "no-trailing-whitespace": false
    },
    "rulesDirectory": []
}
