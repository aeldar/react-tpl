{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-react"
  ],
  "jsRules": {
    "quotemark": [
      true,
      "single",
      "jsx-double",
      "avoid-escape"
    ],
    "trailing-comma": [true, {"multiline": "never", "singleline": "never"}],
    "object-literal-sort-keys": false
  },
  "rules": {
    "import-name": false,
    "variable-name": false,
    "ter-arrow-parens": [
      true,
      "as-needed"
    ],
    "object-literal-sort-keys": false,
    "strict-boolean-expressions": false,
    "ordered-imports": false,
    "interface-name": false,
    "no-console": [
      true,
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "member-access": false,
    "indent": [
      true,
      "spaces"
    ],
    "jsx-boolean-value": false,
    "jsx-no-multiline-js": false,
    "max-line-length": [
      true,
      140
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double",
      "avoid-escape"
    ]
  }
}
